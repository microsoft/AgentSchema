{
  "name": "agentschema",
  "version": "1.0.0-beta.7",
  "description": "TypeScript SDK for declarative agent schema definitions",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agent",
    "schema",
    "agentschema",
    "declarative",
    "ai",
    "typescript"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/AgentSchema"
  },
  "bugs": {
    "url": "https://github.com/microsoft/AgentSchema/issues"
  },
  "homepage": "https://microsoft.github.io/AgentSchema/",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}